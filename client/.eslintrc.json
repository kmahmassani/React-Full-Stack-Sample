{
    "extends": [
        "airbnb"
    ],
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js"
                ]
            }
        ]
    },
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true
    }
}